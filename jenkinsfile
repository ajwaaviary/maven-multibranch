pipeline
{
    agent any
    stages
    {
        stage ('Staged to build Branch1')
        {
            steps
            {
                git 'https://github.com/ajwaaviary/maven-new.git'
            }
        }
        stage('Build-Branch1')
        {
            steps
            {
                sh 'mvn package'
            }
        }
    } 
}
